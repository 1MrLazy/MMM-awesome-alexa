{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "member-variable-declaration",
            "property-declaration"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "no-namespace": [
            false
        ],
        "max-line-length": [
            false
        ],
        "array-type": [
            "array"
        ],
        "no-unused-variable": [
            true
        ],
        "member-access": [
            true
        ],
        "indent": [
            true,
            "spaces"
        ],
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false
    }
}